{
    "root": true,
    "extends": [
        "airbnb-typescript/base",
        "prettier"
    ],
    "plugins": [
        "import",
        "prettier"
    ],
    "parserOptions": {
        "project": "./tsconfig.eslint.json"
    },
    "rules": {
        "prettier/prettier": "error",
        "@typescript-eslint/naming-convention": "off"
    }
}